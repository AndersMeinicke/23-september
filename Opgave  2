class Main{
  public static boolean happy = true;
  public static int a = 13;
  public static int b = 17;
  public static String smallLetters = "I want them large";

public static void main(String [] args) {
   if (iAmHappy(happy))
   {
     System.out.println("I clap my hands");
   }
   else
   {
     System.out.println("I don't clap my hands"); 
   }
   System.out.println(numbers(a,b));
   System.out.println(words(smallLetters));
   System.out.println(wordChecker(smallLetters));
}


public static boolean iAmHappy(boolean happiness)
{
  // fill out what is missing here:
	if(happy == false){
		return false;
	}
	else {
		return true;
}
}
public static int numbers(int i, int j){
int sum = i + j;
return sum;
}
public static String words(String largeLetters){
String itIsLarge = largeLetters;
String finishIt = itIsLarge.toUpperCase();
return finishIt;
}
public static boolean wordChecker(String checker)
{
	char firstLetter = checker.charAt(0);
	boolean isItCapitalized = Character.isUpperCase(firstLetter);
	return isItCapitalized;

}
}
